<!DOCTYPE html>
<html lang="pt-br">

<head>
    <title>Login</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" href="/images/icons/logo_icone.ico" type="image/png">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/bootstrap/css/bootstrap.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/fonts/iconic/css/material-design-iconic-font.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/animsition/css/animsition.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/css/main.css">
    <link rel="stylesheet" type="text/css" href="/css/util.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/sweetalert2@11.7.31/dist/sweetalert2.min.css">

</head>

<body>
    <script src="/js/valida.js"></script>
    <script src="/js/passw.js"></script>
    <script src="/js/rotas.js"></script>
    <div class="limiter">
        <div class="container-login100">
            <div class="wrap-login100">
                <img class="img_logo" src="/images/logos/logo_transparent.png" />
                <form class="login100-form validate-form" method="post" action="log">
                    <span class="login100-form-title p-b-26">
                        INICIAR SESSÃO
                        <br />
                        <br />
                    </span>

                    <div class="wrap-input100 validate-input">
                        <input class="input100" type="text" name="usuario" id="usuario" onblur="return validaCampos()">
                        <span class="focus-input100" data-placeholder="Insira o seu nome de usuário">
                            <br /><br /></span>
                    </div>

                    <div class="wrap-input100 validate-input">
                        <span class="btn-show-pass">
                            <i class="zmdi zmdi-eye"></i>
                        </span>
                        <input class="input100" type="password" name="senha" id="senha" onblur="return validaCampos()">
                        <span class="focus-input100" data-placeholder="Insira a sua senha"> <br /> <br /></span>
                    </div>

                    <div class="container-login100-form-btn">
                        <div class="wrap-login100-form-btn">
                            <div class="login100-form-bgbtn"></div>
                            <button type="submit" class="login100-form-btn" onblur="return validaCampos()">
                                FAZER LOGIN
                            </button>
                        </div>
                    </div>
                </form>

                <div class="container-login100-form-btn">
                    <div class="wrap-login200-form-btn">
                        <div class="login200-form-bgbtn"></div>
                        <button type="button" id="btn-cadastro" class="login200-form-btn"
                            onblur="return rotasCadastro()">
                            Não tenho cadastro
                        </button>
                    </div>
                </div>

                <div class="container-login300-form-btn">
                    <div class="wrap-login300-form-btn">
                        <div class="login300-form-bgbtn"></div>
                        <button type="button" class="login300-form-btn">
                            Esqueci a minha senha
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div id="dropDownSelect1"></div>

    <!--===============================================================================================-->
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11.7.31/dist/sweetalert2.all.min.js"></script>
    <!--===============================================================================================-->
    <script src="/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="/animsition/js/animsition.min.js"></script>
    <!--===============================================================================================-->
    <script src="/bootstrap/js/popper.js"></script>
    <script src="/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="/js/main.js"></script>
</body>

</html>
